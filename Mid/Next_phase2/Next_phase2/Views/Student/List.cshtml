@model List<Next_phase2.Models.Student>
@{
    ViewBag.Title = "List";
}

<h2>List</h2>
<table class="table table-condensed">
    <tr>
        <th>Name</th>
        <th>Id</th>
        <th>Dob</th>
    </tr>

    @foreach (var item in Model)
{
    <tr>
        <td>@item.Name</td>
        <td>@item.Id</td>
        <td>@item.Dob</td>
    </tr>
}
</table>
